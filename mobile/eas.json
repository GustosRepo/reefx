{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Release",
        "scheme": "REEFX",
        "releaseChannel": "prod"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "admin@code-wrx.com"
      }
    }
  }
}